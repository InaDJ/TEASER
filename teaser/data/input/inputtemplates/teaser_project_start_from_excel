from teaser.project import Project
import teaser.logic.utilities as utilities
import os

def create_project_from_excel():
    '''
    Instantiate a Project class (with load_data set to true), then instantiate a
    Building class, with the project as a parent. This automatically adds the
    specific building and all its future changes to the project.
    '''
    prj = Project(load_data=True,used_data_country="${used_data_country}")
    prj.name = "${project_name}"
